{
  "name": "eslint-config-tsyirvo-react-native",
  "version": "1.5.0",
  "description": "Custom ESLint config for my React Native projects",
  "main": "index.js",
  "scripts": {
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsyirvo/eslint-config-tsyirvo-react-native.git"
  },
  "bugs": {
    "url": "https://github.com/tsyirvo/eslint-config-tsyirvo-react-native/issues"
  },
  "keywords": [
    "React Native",
    "ESLint",
    "ESLint config",
    "Typescript"
  ],
  "author": "Gabriel Hofman <gabriel.hofman.00@gmail.com>",
  "license": "ISC",
  "engines": {
    "yarn": ">=3",
    "node": ">=20.8.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-plugin-i18n-json": "4.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.1.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-unicorn": "51.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "eslint-plugin-filename-rules": "^1.3.1",
    "husky": "9.0.11",
    "semantic-release": "23.0.6",
    "semantic-release-cli": "5.4.6"
  },
  "resolutions": {
    "underscore.string": "2.4.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        2,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        2,
        "always",
        "Infinity"
      ],
      "header-max-length": [
        2,
        "always",
        "Infinity"
      ]
    }
  },
  "peerDependencies": {
    "eslint": "8.57.0",
    "typescript": "5.3.0"
  }
}
